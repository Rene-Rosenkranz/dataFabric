{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET",
							"additionalHeaders": "Authorization: Basic dGVzdDpyZ2FvZGlob2kyNm1ydml6b3kycWd0bzV5ZzJ3eGVwa2d4bnV6ZnU0YmxvNWljaGYyNXVh",
							"requestTimeout": ""
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "SqlServerSink",
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true,
							"keys": [
								"IterationID"
							]
						},
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['count']"
								}
							},
							{
								"source": {
									"path": "$['value'][0]['id']"
								},
								"sink": {
									"name": "IterationID",
									"type": "Guid"
								}
							},
							{
								"source": {
									"path": "$['value'][0]['name']"
								},
								"sink": {
									"name": "IterationName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['value'][0]['path']"
								},
								"sink": {
									"name": "IterationPath",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['value'][0]['attributes']['startDate']"
								},
								"sink": {
									"name": "StartDate",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "$['value'][0]['attributes']['finishDate']"
								},
								"sink": {
									"name": "FinishDate",
									"type": "DateTime"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "DevOpsIterations",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Iterations",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}