{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "devOpsSync"
		},
		"HttpServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'HttpServer1'"
		},
		"ThesisDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ThesisDB'"
		},
		"Azure DevOps_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Azure DevOps'"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dev.azure.com/renerosenkranztt0058"
		},
		"HttpServer1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "tewst"
		},
		"Azure DevOps_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dev.azure.com/renerosenkranztt0058/"
		},
		"Azure DevOps_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "tset"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "devOpsSync_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"HttpServer1_password": {
						"value": "[parameters('HttpServer1_password')]"
					},
					"ThesisDB_connectionString": {
						"value": "[parameters('ThesisDB_connectionString')]"
					},
					"Azure DevOps_password": {
						"value": "[parameters('Azure DevOps_password')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"HttpServer1_properties_typeProperties_userName": {
						"value": "[parameters('HttpServer1_properties_typeProperties_userName')]"
					},
					"Azure DevOps_properties_typeProperties_url": {
						"value": "[parameters('Azure DevOps_properties_typeProperties_url')]"
					},
					"Azure DevOps_properties_typeProperties_userName": {
						"value": "[parameters('Azure DevOps_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}